{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
      feature/update-devcontainer
    "**/.env.*local"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": []
    },
    "type-check": {
      "dependsOn": [
        "^type-check"
      ],
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "test:e2e": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    "**/.env",
    "**/.env.local",
    "**/.env.development.local",
    "**/.env.test.local",
    "**/.env.production.local"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "build/**",
        "lib/**"
      ],
      "env": [
        "NODE_ENV",
        "VERCEL_URL",
        "DATABASE_URL"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": [
        "eslint-report.json",
        ".eslintcache"
      ]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"],
      "cache": false
    },
    "lint:check": {
      "dependsOn": ["^lint:check"],
      "outputs": [
        "eslint-report.json",
        ".eslintcache"
      ]
    },
    "type-check": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "*.tsbuildinfo",
        "tsconfig.tsbuildinfo"
      ]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": [
        "coverage/**",
        "test-results/**",
        "junit.xml"
      ],
      "inputs": [
        "src/**/*.{js,ts,jsx,tsx}",
        "test/**/*.{js,ts,jsx,tsx}",
        "__tests__/**/*.{js,ts,jsx,tsx}",
        "*.config.{js,ts}"
      ]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "test:e2e": {
      "dependsOn": [
        "^build",
        "^dev"
      ],
      "outputs": [
        "test-results/**",
        "playwright-report/**",
        "cypress/screenshots/**",
        "cypress/videos/**"
      ]
    },
    "test:e2e:ui": {
      "cache": false,
      "persistent": true
       main
    },
    "clean": {
      "cache": false
    },
    "db:generate": {
      "cache": false,
      "inputs": [
        "prisma/schema.prisma",
        "prisma/migrations/**"
      ]
    },
    "db:push": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "inputs": [
        "prisma/schema.prisma",
        "prisma/migrations/**"
      ]
    },
    "db:seed": {
      "cache": false,
      "dependsOn": [
        "db:generate"
      ]
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": [
        ".prettierCache"
      ]
    },
    "format:check": {
      "outputs": [
        ".prettierCache"
      ]
    }
  },
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
    feature/update-devcontainer
}

}
  main
