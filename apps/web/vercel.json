{
  "version": 2,
  "framework": "nextjs",
  "installCommand": "cd apps/web && npm install --legacy-peer-deps",
  "buildCommand": "cd apps/web && npm run build",
  "outputDirectory": "apps/web/.next",
  "rootDirectory": ".",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD apps/web",
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_APP_URL": "https://facturacion-autonomos-monorepo.vercel.app",
    "NEXT_PUBLIC_API_BASE_URL": "https://facturacion-autonomos-monorepo.vercel.app/api"
  }
}
