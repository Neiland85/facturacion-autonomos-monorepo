{
  "compilerOptions": {
    "target": "ES2022",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "node20",
    "moduleResolution": "Node16",
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "incremental": true,
    "noUncheckedIndexedAccess": true,
    "tsBuildInfoFile": "dist/tsconfig.tsbuildinfo"
  }
}